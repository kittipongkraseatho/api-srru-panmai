{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "blogs",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "blog_name": {
      "type": "string"
    },
    "blog_dateil": {
      "type": "richtext",
      "default": "<h1>กรอกรายอะเอียดบทความ!</h1>"
    },
    "blog_sub_title": {
      "type": "string"
    },
    "blog_img": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "blog_content": {
      "type": "enumeration",
      "enum": [
        "content_1",
        "content_2",
        "content_3",
        "content_4",
        "content_5",
        "content_6",
        "content_youtube",
        "draft"
      ],
      "default": "draft"
    },
    "blog_youtube": {
      "type": "richtext"
    }
  }
}
